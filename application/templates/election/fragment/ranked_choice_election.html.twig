<tr>
    <th>Results</th>
    <td class="pt-0">
        <table class="table">
            <thead>
            <tr>
                <th class="col-md-9 border-top-0 align-text-top">Show</th>
                <th class="col-md-3 text-end border-top-0 align-text-top">Rank</th>
            </tr>
            </thead>
            <tbody>
            {% for voteTally in voteTallies %}
                <tr>
                    <td class="col-md-9">
                        {{ voteTally.getShowTitle }}
                    </td>
                    <td class="col-md-3 text-end">
                        {{ voteTally.getRank }}
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
        <div class="mt-3 mb-3">
            <a
                    class="btn btn-primary"
                    target="_blank"
                    href="{{ path('admin_election_export', { 'id': election.id }) }}"
            >
                Export vote tally
            </a>
        </div>
        <div class="mt-3 mb-3">
            <a
                    class="btn btn-secondary"
                    target="_blank"
                    href="{{ path('admin_election_export_raw', { 'id': election.id }) }}"
            >
                Export raw votes
            </a>
        </div>
    </td>
</tr>
